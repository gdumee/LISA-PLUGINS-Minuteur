msgid "i_can_plugin"
msgstr  "(10, \"Je peux gérer des minuteurs\"),"
        "(10, \"Je peux faire des minuteurs\"),"

msgid "i_can_setTimer"
msgstr  "(10, \"Je peux démarrer des minuteurs\"),"

msgid "i_can_stopTimer"
msgstr  "(10, \"Je peux arrêter un minuteur\"),"

msgid "i_can_getTimer"
msgstr  "(10, \"Je peux vous donner le temps restant\"),"


msgid "no_duration"
msgstr  "(10, \"Vous n'avez pas indiqué de durée. Je vous écoute\"),"
        "(10, \"Bien sûr, un minuteur de quelle durée ?\"),"
        "(10, \"Pourriez vous me précisez la durée du minuteur ?\"),"
        "(10, \"Pour quelle durée voulez vous lancer ce minuteur ?\"),"
        "(10, \"Sans problème, pour quelle durée ?\"),"

msgid "start_timer"
msgstr  "(5,  \"je lance un minuteur {name} de {duration}\"),"
        "(10, \"je démarre un minuteur de {duration} {name}\"),"
        "(10, \"Le minuteur de {duration} {name} est démarré\"),"
        "(10, \"Le minuteur de {duration} {name} est lancé\"),"
        "(10, \"C'est fait, le minuteur {name} de {duration} est lancé\"),"

msgid "for"
msgstr  "pour"

msgid "left_time"
msgstr  "(10, \"Il reste {duration} {name}\"),"
        "(10, \"{name}, il reste encore {duration}\"),"
        "(5,  \"Il y en a encore {duration} {name}\"),"
        "(10, \"Le minuteur {name} indique {duration} restante\"),"
        "(10, \"Il reste encore {duration} {name}\"),"

msgid "timer_list"
msgstr  "(10, \"Les minuteurs en cours sont\"),"
        "(10, \"Voici les minuteurs en cours\"),"
        "(10, \"Voici les minuteurs que je connais\"),"
        "(10, \"Je connais les minuteurs\"),"
        "(10, \"Les minuteurs actifs sont\"),"

msgid "unknown_timer"
msgstr  "(10, \"Je ne connais pas ce minuteur\"),"
        "(10, \"Je ne reconnais pas ce minuteur\"),"
        "(10, \"Je n'ai pas de minuteur de ce nom\"),"
        "(5,  \"Ce minuteur m'est inconnu\"),"
        "(10, \"Ce minuteur ne me dit rien\"),"

msgid "timer_over"
msgstr  "(10, \"Le minuteur {name} est terminé\"),"
        "(10, \"Je vous informe que le minuteur {name} est terminé\"),"
        "(10, \"Le minuteur {name} est fini\"),"
        "(10, \"Excusez moi, le minuteur {name} est terminé\"),"
        "(10, \"Excusez moi, le minuteur {name} est fini\"),"

msgid "stop_timer"
msgstr  "(10, \"J'ai arreté le minuteur {name}\"),"
        "(10, \"Le minuteur {name} a été arrêté\"),"
        "(10, \"J'arrête le minuteur {name}\"),"
        "(10, \"J'ai annulé le minuteur {name}\"),"
        "(10, \"J'ai bien arrêté le minuteur {name}\"),"

msgid "no_timer"
msgstr  "(10, \"Je n'ai pas de minuteur en cours\"),"
        "(10, \"Il n'y a aucun minuteur en cours\"),"
        "(10, \"Je n'ai aucun minuteur\"),"
        "(10, \"Vous n'avez aucun minuteur actif\"),"
        "(10, \"Il n'y a aucun minuteur actif\"),"

msgid "ended_timer"
msgstr  "(10, \"Le miniteur {name} est terminé\"),"
        "(10, \"Le miniteur {name} est déjà terminé\"),"
        "(10, \"Ce miniteur est déjà terminé\"),"
        "(10, \"Ce miniteur est terminé\"),"
        "(2,  \"Ce miniteur n'est plus en cours\"),"

