msgid "I start a timer for"
msgstr  "(5,  \"je lance un minuteur de {0}\"),"
        "(10, \"je démarre un minuteur de {0}\"),"
        "(10, \"je compte à rebours {0}\"),"
        "(10, \"je chronomètre {0}\")"

msgid "for"
msgstr "pour"

msgid "There is remaining"
msgstr  "(10, \"Il reste {0}\"),"
        "(10, \"Il reste encore {0}\"),"
        "(5,  \"Il y en a encore pour {0}\")"

msgid "existing timer"
msgstr  "(10,\"Le minuteur en cours est {slist}.\"),"
"(10,\"Le minuteur actif est {slist}.\"),"

msgid "existing timers"
msgstr  "(10,\"Les minuteurs en cours sont {slist}.\"),"
"(10,\"Les minuteurs actifs sont {slist}.\"),"

msgid "I don't know this timer"
msgstr  "Je ne connais pas ce minuteur."


msgid "The timer is over"
msgstr  "Le minuteur {0} est terminé"

msgid "I stop timer"
msgstr  "(10, \"J'ai arreté le minuteur {name}\"),"
        "(10, \"J'arrête le minuteur\")"

